{
  "version": 2,
  "builds": [
    {
      "src": "discord-profile-creator.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "DISCORD_CLIENT_SECRET": "@discord-client-secret"
  }
}
